import lombok.NoArgsConstructor;

@Data
@NoArgsConstructor
@AllArgsConstructor

public class CartResponse {

	private Long id;
	private double cartTotal;
	private LocalDate cartDate;
	private  Customer customer;
	private List<CartItemResponse> cartItems;
	
}
